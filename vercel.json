{
    "version": 2,
    "buildCommand": "pip install -r requirements.txt",
    "devCommand": "uvicorn api.index:app --reload",
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}
